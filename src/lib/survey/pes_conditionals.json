{
	"title": "Patient Experience Questionnaire",
	"logo": "data:image/png;base64,",
	"logoHeight": "80",
	"pages": [
		{
			"name": "survey",
			"title": "OVERALL EXPERIENCE",
			"elements": [
				{
					"type": "rating",
					"name": "overallExperienceRating",
					"title": "How would you rate your overall experience at our facility?",
					"isRequired": true,
					"rateType": "smileys",
					"rateCount": 10,
					"rateMax": 10
				},
				{
					"type": "checkbox",
					"name": "aspectContributionOverallRating",
					"visibleIf": "{overallExperienceRating} <= 5",
					"title": "What aspects contributed to your rating?",
					"choices": [
						{
							"value": "admissionProcessAspect",
							"text": "Admission process"
						},
						{
							"value": "facilityCleanlinessAspect",
							"text": "Facility cleanliness"
						},
						{
							"value": "dischargeProcessAspect",
							"text": "Discharge process"
						},
						{
							"value": "staffInteractionAspect",
							"text": "Staff interaction"
						}
					]
				},
				{
					"type": "panel",
					"name": "admissionProcessPanel",
					"visibleIf": "{aspectContributionOverallRating} contains 'admissionProcessAspect' and {aspectContributionOverallRating} <> ['admissionProcessAspect', 'facilityCleanlinessAspect', 'dischargeProcessAspect', 'staffInteractionAspect']",
					"title": "Admission Process",
					"elements": [
						{
							"type": "rating",
							"name": "admissionEfficiency",
							"title": "How efficient was the admission/registration process?",
							"autoGenerate": false,
							"rateValues": [1, 2, 3, 4, 5]
						},
						{
							"type": "rating",
							"name": "admissionStaffHelpfulness",
							"visibleIf": "{admissionEfficiency} notempty",
							"title": "Were the admission staff helpful and courteous?",
							"autoGenerate": false,
							"rateValues": [1, 2, 3, 4, 5]
						},
						{
							"type": "radiogroup",
							"name": "insurancePaymentInfo",
							"visibleIf": "{admissionStaffHelpfulness} notempty",
							"title": "Was insurance and payment information clearly explained?",
							"choices": ["Yes", "No"]
						},
						{
							"type": "dropdown",
							"name": "waitingTime",
							"visibleIf": "{insurancePaymentInfo} notempty",
							"title": "How long did you wait before being attended to?",
							"choices": ["<5 minutes", "5-10 minutes", "10-20 minutes", "More than 20 minutes"]
						}
					]
				},
				{
					"type": "panel",
					"name": "dischageProcessPanel",
					"visibleIf": "{aspectContributionOverallRating} contains 'dischargeProcessAspect' and {aspectContributionOverallRating} <> ['admissionProcessAspect', 'facilityCleanlinessAspect', 'dischargeProcessAspect', 'staffInteractionAspect']",
					"title": "Discharge Process\n",
					"elements": [
						{
							"type": "radiogroup",
							"name": "dischargeInstructions",
							"title": "Were discharge instructions clearly explained?",
							"choices": ["Not at all", "Somewhat", "Neutral", "Mostly", "Completely"]
						},
						{
							"type": "dropdown",
							"name": "medicationInstructions",
							"visibleIf": "{dischargeInstructions} notempty",
							"title": "Do you understand your medication instructions?",
							"choices": ["No", "Yes, somewhat", "Yes, completely"]
						},
						{
							"type": "radiogroup",
							"name": "followUpCare",
							"visibleIf": "{medicationInstructions} notempty",
							"title": "Was follow-up care explained thoroughly?",
							"choices": ["Not at all", "Somewhat", "Neutral", "Mostly", "Completely"]
						},
						{
							"type": "radiogroup",
							"name": "dischargeEfficiency",
							"visibleIf": "{followUpCare} notempty",
							"title": "Was the discharge process efficient?",
							"choices": [
								"Very inefficient",
								"Somewhat inefficient",
								"Neutral",
								"Somewhat efficient",
								"Very efficient"
							]
						}
					]
				},
				{
					"type": "panel",
					"name": "facilityEnvironmentPanel",
					"visibleIf": "{aspectContributionOverallRating} contains 'facilityCleanlinessAspect' and {aspectContributionOverallRating} <> ['admissionProcessAspect', 'facilityCleanlinessAspect', 'dischargeProcessAspect', 'staffInteractionAspect']",
					"title": "Facility and Environment",
					"elements": [
						{
							"type": "matrix",
							"name": "facilityCleanliness",
							"title": "How clean were the facilities (rooms, bathrooms, common areas)?",
							"columns": [
								{
									"value": 1,
									"text": "Very Dirty"
								},
								{
									"value": 2,
									"text": "Dirty"
								},
								{
									"value": 3,
									"text": "Neutral"
								},
								{
									"value": 4,
									"text": "Clean"
								},
								{
									"value": 5,
									"text": "Very Clean"
								}
							],
							"rows": [
								{
									"value": "rooms",
									"text": "Rooms"
								},
								{
									"value": "bathrooms",
									"text": "Bathrooms"
								},
								{
									"value": "commonAreas",
									"text": "Common Areas"
								}
							]
						},
						{
							"type": "rating",
							"name": "roomComfort",
							"visibleIf": "{facilityCleanliness.bathrooms} notempty and {facilityCleanliness.commonAreas} notempty and {facilityCleanliness.rooms} notempty",
							"title": "How would you rate the comfort of your room/ward?",
							"autoGenerate": false,
							"rateValues": [1, 2, 3, 4, 5]
						},
						{
							"type": "radiogroup",
							"name": "noiseLevel",
							"visibleIf": "{roomComfort} notempty",
							"title": "Was the noise level acceptable?",
							"choices": ["Yes", "No"]
						}
					]
				},
				{
					"type": "panel",
					"name": "staffInteractionPanel",
					"visibleIf": "{aspectContributionOverallRating} contains 'staffInteractionAspect' and {aspectContributionOverallRating} <> ['admissionProcessAspect', 'facilityCleanlinessAspect', 'dischargeProcessAspect', 'staffInteractionAspect']",
					"title": "Staff Interaction",
					"elements": [
						{
							"type": "dropdown",
							"name": "staffRespect",
							"title": "Did staff treat you with respect and dignity?",
							"choices": ["No", "Yes, sometimes", "Yes, always"]
						},
						{
							"type": "radiogroup",
							"name": "staffListen",
							"visibleIf": "{staffRespect} notempty",
							"title": "Did staff listen to your concerns?",
							"choices": ["Not at all", "Somewhat", "Neutral", "Mostly", "Completely"]
						},
						{
							"type": "radiogroup",
							"name": "staffAvailability",
							"visibleIf": "{staffListen} notempty",
							"title": "Were staff available when needed?",
							"choices": [
								"Not available",
								"Rarely available",
								"Sometimes available",
								"Usually available",
								"Always available"
							]
						}
					]
				},
				{
					"type": "panel",
					"name": "areasNeedImprovementPanel",
					"visibleIf": "{aspectContributionOverallRating} allof ['admissionProcessAspect', 'facilityCleanlinessAspect', 'dischargeProcessAspect', 'staffInteractionAspect']",
					"title": "We are sorry you had a poor experience with us",
					"description": "Please share your experience",
					"elements": [
						{
							"type": "comment",
							"name": "areasForImprovement",
							"title": "What areas need improvement?"
						}
					]
				},
				{
					"type": "rating",
					"name": "medicalCareRating",
					"visibleIf": "{aspectContributionOverallRating} <> ['admissionProcessAspect', 'facilityCleanlinessAspect', 'dischargeProcessAspect', 'staffInteractionAspect']",
					"title": "How would you rate the quality of medical care from our doctors and nurses?",
					"isRequired": true,
					"rateType": "smileys",
					"rateCount": 10,
					"rateMax": 10
				},
				{
					"type": "panel",
					"name": "medicalCarePanel",
					"visibleIf": "{medicalCareRating} <= 6",
					"title": "Medical care",
					"elements": [
						{
							"type": "radiogroup",
							"name": "whoProvidedPoorCare",
							"visibleIf": "{medicalCareRating} <= 6",
							"title": "Who provided poor care?",
							"choices": [
								{
									"value": "poorCareDoctor",
									"text": "Doctors"
								},
								{
									"value": "poorCareNurse",
									"text": "Nurses"
								},
								{
									"value": "poorCareBoth",
									"text": "Both"
								}
							]
						},
						{
							"type": "panel",
							"name": "nursesPanel",
							"visibleIf": "{whoProvidedPoorCare} anyof ['poorCareNurse', 'poorCareBoth']",
							"title": "Nurses care",
							"elements": [
								{
									"type": "dropdown",
									"name": "nurseStaffResponse",
									"title": "Did nursing staff respond promptly to your needs?",
									"choices": ["No", "Yes, sometimes", "Yes, always"]
								},
								{
									"type": "radiogroup",
									"name": "nurseCheck",
									"visibleIf": "{nurseStaffResponse} notempty",
									"title": "Did nurses regularly check on you?",
									"choices": ["Not at all", "Rarely", "Occasionally", "Yes, frequently"]
								}
							]
						},
						{
							"type": "panel",
							"name": "doctorsPanel",
							"visibleIf": "{whoProvidedPoorCare} anyof ['poorCareBoth', 'poorCareDoctor']",
							"title": "Doctors care",
							"elements": [
								{
									"type": "radiogroup",
									"name": "procedureExplanation",
									"title": "Were procedures and treatments clearly explained?",
									"choices": ["Not explained", "Confusing", "Somewhat clear", "Very clear"]
								},
								{
									"type": "radiogroup",
									"name": "medicationExplanation",
									"visibleIf": "{procedureExplanation} notempty",
									"title": "Were medications explained clearly?",
									"choices": ["Very Poor", "Poor", "Neutral", "Good", "Excellent"]
								}
							]
						}
					]
				},
				{
					"type": "rating",
					"name": "staffRating",
					"visibleIf": "{aspectContributionOverallRating} <> ['admissionProcessAspect', 'facilityCleanlinessAspect', 'dischargeProcessAspect', 'staffInteractionAspect']",
					"title": "How would you rate the attentiveness and professionalism of our staff?",
					"isRequired": true,
					"rateType": "smileys",
					"rateCount": 10,
					"rateMax": 10
				},
				{
					"type": "panel",
					"name": "professionalismPanel",
					"visibleIf": "{staffRating} <= 6",
					"title": "Attentiveness and professionalism",
					"elements": [
						{
							"type": "radiogroup",
							"name": "staffCheck",
							"title": "Did staff regularly check on you?",
							"choices": ["Not at all", "Rarely", "Occasionally", "Yes, frequently"]
						},
						{
							"type": "dropdown",
							"name": "medicalStaffResponse",
							"visibleIf": "{staffCheck} notempty",
							"title": "Did medical staff respond promptly to your needs?",
							"choices": ["No", "Yes, sometimes", "Yes, always"]
						}
					]
				},
				{
					"type": "comment",
					"name": "goodAspects",
					"visibleIf": "{medicalCareRating} >= 9 and {overallExperienceRating} >= 9 and {staffRating} >= 9",
					"title": "What aspects of your care were particularly good?"
				}
			]
		}
	],
	"showPageTitles": false,
	"showProgressBar": true,
	"progressBarLocation": "bottom",
	"autoAdvanceEnabled": true,
	"firstPageIsStartPage": true,
	"questionsOnPageMode": "singlePage",
	"widthMode": "static"
}
